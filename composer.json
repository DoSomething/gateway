{
  "name": "dosomething/gateway",
  "description": "Standard PHP API client for DoSomething.org services.",
  "require": {
    "nesbot/carbon": "~1.14",
    "giggsey/libphonenumber-for-php": "~7.0",
    "lcobucci/jwt": "^3.1",
    "guzzlehttp/guzzle": "^6.2.1",
    "league/oauth2-client": "~2.2",
    "symfony/psr-http-message-bridge": "^1.0",
    "zendframework/zend-diactoros": "^1.3",
    "gree/jose": "^2.2",
    "fzaninotto/faker": "^1.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "illuminate/database": "^5.5.0",
    "illuminate/http": "^5.5.0",
    "symfony/var-dumper": "^3.1"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "David Furnes",
      "email": "dfurnes@dosomething.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "DoSomething\\Gateway\\": "src/"
    },
    "files": ["src/Laravel/helpers.php"]
  },
  "autoload-dev": {
    "classmap": ["tests/TestCase.php"],
    "psr-4": {
      "DoSomething\\GatewayTests\\": "tests/"
    }
  }
}
